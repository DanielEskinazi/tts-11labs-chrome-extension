{
  "manifest_version": 3,
  "name": "ElevenLabs Reader",
  "version": "1.0.0",
  "description": "Convert text to speech using ElevenLabs AI voices",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "ElevenLabs Reader"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
